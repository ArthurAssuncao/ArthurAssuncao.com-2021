{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "standard-version",
    "release:first": "yarn release -- --first-release",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "new:feature": "git checkout -b ",
    "merge": "git merge --no-ff ",
    "merge:to:develop": "git checkout develop && git merge --no-ff ",
    "pr": "gh pr create --base main && git branch -D `git branch | grep -E '(feature)'`"
  },
  "dependencies": {
    "@formspree/react": "^2.2.3",
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "html-react-parser": "^1.2.8",
    "next": "11.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.2.24",
    "next-seo": "^4.26.0",
    "normalize-scss": "^7.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-id-generator": "^3.0.1",
    "react-scroll": "^1.8.3",
    "react-swipeable": "^6.2.0",
    "react-toastify": "^8.0.2",
    "react-vertical-timeline-component": "^3.3.3",
    "react-youtube": "^7.13.1",
    "sass": "^1.37.5",
    "typescript": "^4.4.2",
    "worker-timers": "^7.0.37",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@iconify/icons-bx": "^1.1.2",
    "@iconify/icons-cil": "^1.1.0",
    "@iconify/icons-fa-brands": "^1.1.1",
    "@iconify/icons-ic": "^1.1.9",
    "@iconify/icons-logos": "^1.1.10",
    "@iconify/icons-mdi": "^1.1.22",
    "@iconify/icons-ri": "^1.1.0",
    "@iconify/icons-simple-icons": "^1.1.40",
    "@iconify/icons-vscode-icons": "^1.1.6",
    "@iconify/react": "^3.0.1",
    "@types/react": "17.0.17",
    "@types/react-scroll": "^1.8.3",
    "@types/react-vertical-timeline-component": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.20.1",
    "typescript-eslint-parser": "^22.0.0",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
